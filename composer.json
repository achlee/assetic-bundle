{
  "name": "coursehero/assetic-bundle",
  "homepage": "https://github.com/coursehero/assetic-bundle",
  "description": "Course Hero's Assetic tools bundle",
  "keywords": [
    "assetic",
    "source maps",
    "cache busting",
    "symfony"
  ],
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Connor Clark",
      "email": "cjamcl@gmail.com"
    },
    {
      "name": "Jason Wentworth",
      "email": "jwentworth@coursehero.com"
    }
  ],
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "require": {
    "kriswallsmith/assetic": "~1.2|~1.4",
    "php": ">=7.0"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2.1",
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "^5.7",
    "symfony/assetic-bundle": "2.8.2"
  },
  "autoload": {
    "psr-4": {
      "CourseHero\\AsseticBundle\\": "src/"
    },
    "files": ["src/Utils.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "CourseHero\\AsseticBundle\\Tests\\": "tests/"
    },
    "files": ["src/Utils.php"]
  }
}
